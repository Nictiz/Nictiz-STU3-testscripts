<?xml version="1.0" encoding="UTF-8"?>
<TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="server">
   <id value="mp9-ma-fhir3-0-2-xis-6-13-xml"/>
   <name value="Medication Process 9.0.7 - MedicationAgreement (NL: MedicatieAfspraak) - XIS Server - Scenario 6.13 - XML Format"/>
   <description value="Scenario 6.13 - Toedieningssnelheid"/>
   <nts:patientTokenFixture href="mp9-nl-core-patient-XXX-MAASZ-token.xml" />
   <profile id="bundle-profile">
      <reference value="http://hl7.org/fhir/StructureDefinition/Bundle"/>
   </profile>
   <test id="mg-mgr-mg-MA-Scenarioset6-6-13optioneel">
      <name value="Scenario 6.13"/>
      <description value="Scenario 6.13 - Toedieningssnelheid"/>
      <nts:include value="xis-ma-vv-operation-search">
         <nts:variable name="description"
                       value="Test XIS server to serve MedicationAgreement (NL: MedicatieAfspraak) - Toedieningssnelheid"/>
         <nts:variable name="accept" value="xml"/>
         <nts:variable name="params"
            value="?category=http://snomed.info/sct|16076005&amp;medication.code=urn:oid:2.16.840.1.113883.2.4.4.10|94692&amp;_include=MedicationRequest:medication"/>
      </nts:include>
      <nts:include value="assert-responseSearchBundleSuccess" scope="common"/>
      <nts:include value="assert-responseBundleContent" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:variable name="description"
                       value="Confirm that the returned searchset Bundle contains 1 MedicationAgreement."/>
         <nts:variable name="expression"
                       value="Bundle.entry.where(resource.is(MedicationRequest)).count() = 1"/>
      </nts:include>
   </test>
</TestScript>
