   <test id="01-RetrieveWoman">
      <name value="01-RetrieveWoman"/>
      <description value="Test Client query operation for Woman (Patient)"/>
      <action>
         <operation>
            <type>
               <system value="http://hl7.org/fhir/testscript-operation-codes"/>
               <code value="search"/>
            </type>
            <resource value="Patient"/>
            <description value="Test Client query operation for Woman (Patient)"/>
            <params value="?_id=${patient-id}"/>
            <requestHeader>
               <field value="Authorization"/>
               <value value="${patient-token-id}"/>
            </requestHeader>
         </operation>
      </action>
      <action>
         <assert>
            <description value="Confirm that HTTP header Authorization contains the patient token ${patient-token-id}"/>
            <direction value="request"/>
            <headerField value="Authorization"/>
            <value value="${patient-token-id}"/>
